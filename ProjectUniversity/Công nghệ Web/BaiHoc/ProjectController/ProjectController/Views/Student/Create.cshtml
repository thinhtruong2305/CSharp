@model ProjectController.Models.Student
@{
    Layout = null;
}
<Reference Include="System" />
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <h1>Create</h1>
    <div>
        <form method="post" action="~/Student/Create">
            studentID: <input type="text" name="txtID" /><br />
            firstName: <input type="text" name="txtFName" /><br />
            lastName:  <input type="text" name="txtLName" /><br />
            <input type="submit" value="submit" />
        </form>
        @if (Model != null)
        {
            <h1> @Model.studentID</h1>
            <h1> @Model.firstName</h1>
            <h1> @Model.lastName</h1>
        }
    </div>
</body>
</html>

